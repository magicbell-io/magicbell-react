    "babel-loader": "^8.2.2",
    "codecov": "^3.8.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "faker": "^5.4.0",
    "husky": "^7.0.2",
    "jest": "^27.1.1",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^10.5.4",
    "microbundle": "^0.13.3",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rosie": "^2.0.1",
    "sinon": "^11.1.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@magicbell/react-headless": "^2.3.4",
    "@tippyjs/react": "^4.2.4",
    "ably": "^1.2.13",
