    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.0.2",
    "@types/faker": "^5.1.7",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.3",
